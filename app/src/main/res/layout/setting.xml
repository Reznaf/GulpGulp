<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ScrollView
        android:id="@+id/rf92gekuadr"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:background="#FF25253A"
        android:paddingTop="83dp"
        android:paddingHorizontal="22dp"
        android:fillViewport="true">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <LinearLayout
                android:id="@+id/rrt5ltp5vq8g"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:layout_marginBottom="71dp"
                android:orientation="horizontal">
                <ImageView
                    android:id="@+id/back_button"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="16dp"
                    android:src="@drawable/back"
                    android:scaleType="fitXY"
                    />

                <TextView
                    android:id="@+id/r244kvnuy3s8"
                    android:textColor="#FFFFFF"
                    android:textSize="36sp"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:layout_weight="1"
                    android:text="Settings"
                    android:gravity="center_vertical"
                    android:fontFamily="@font/font_bold"
                    />
            </LinearLayout>
            <TextView
                android:id="@+id/rmrxnwlp3ja"
                android:textColor="#C5CFF8"
                android:textSize="16sp"
                android:layout_marginBottom="14dp"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:text="Notification Settings"
                android:fontFamily="@font/font_bold"

                />
            <LinearLayout
                android:id="@+id/r6lhxl2hbc8q"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:layout_marginBottom="18dp"
                android:orientation="horizontal">
                <TextView
                    android:id="@+id/rlkc58k9o84"
                    android:textColor="#FFFFFF"
                    android:textSize="16sp"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:layout_weight="1"
                    android:text="Enable Notification"
                    android:fontFamily="@font/font_medium"

                    />
                <LinearLayout
                    android:id="@+id/rq26hbqq26xg"
                    android:layout_width="140dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical">
                </LinearLayout>
                        <Switch
                            android:id="@+id/round_up_switch"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:paddingStart="20dp"
                            android:paddingEnd="16dp"
                            android:checked="true"
                            android:layout_weight="1"
                            />

            </LinearLayout>
            <TextView
                android:id="@+id/ryftvur7z9h"
                android:textColor="#FFFFFF"
                android:textSize="16sp"
                android:layout_marginBottom="16dp"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:text="Set Reminder"
                android:fontFamily="@font/font_medium"

                />
            <TextView
                android:id="@+id/rh2w31sju94l"
                android:textColor="#C5CFF8"
                android:textSize="16sp"
                android:layout_marginBottom="20dp"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:text="General Settings"
                android:fontFamily="@font/font_bold"
                />

            <TextView
                android:id="@+id/ru57l5l181k"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="35dp"
                android:fontFamily="@font/font_medium"
                android:text="Reset data"
                android:textColor="#FFFFFF"
                android:textSize="16sp"

                />

            <TextView
                android:id="@+id/change_button"
                android:textColor="#4461E5"
                android:textSize="16sp"
                android:layout_marginBottom="14dp"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:text="Change Account"
                android:fontFamily="@font/font_bold"
                />
            <TextView
                android:id="@+id/logout_button"
                android:textColor="#EE2B2B"
                android:textSize="16sp"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:text="Log Out"
                android:fontFamily="@font/font_bold"

                />
        </LinearLayout>
    </ScrollView>
</LinearLayout>